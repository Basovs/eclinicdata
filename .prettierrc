{
  'tabWidth': 2,
  'bracketSameLine': false,
  'semi': false,
  'printWidth': 100,
  'singleQuote': true,
  'trailingComma': 'all',
  'bracketSpacing': true,
  'arrowParens': 'avoid',
  'embeddedLanguageFormatting': 'off',
  'endOfLine': 'auto',
  'importOrderSeparation': true,
  'importOrderSortSpecifiers': true,
  'importOrder': ['<THIRD_PARTY_MODULES>', '^@/(.*)$', '^[../]', '^[./]'],
  'plugins': ['@trivago/prettier-plugin-sort-imports'],
}
